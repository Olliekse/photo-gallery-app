{
  "name": "citjs-photo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "vercel-build": "prisma migrate deploy && next build",
    "db:seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' -r tsconfig-paths/register prisma/seed.ts",
    "db-reset": "prisma migrate reset --force --preview-feature",
    "db:migrate:deployment": "[[ \"$APP_ENV\" == \"preview\" ]] && yarn db:reset || yarn db:migrate:prod",
    "db:migrate:prod": "prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.27.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@faker-js/faker": "^10.0.0",
    "@prisma/client": "^6.16.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "framer-motion": "^12.23.22",
    "next": "15.5.0",
    "prisma": "^6.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.64.0",
    "react-icons": "^5.5.0",
    "swr": "^2.3.6",
    "typescript": "^5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
